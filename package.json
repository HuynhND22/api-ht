{
  "name": "node-express-ts",
  "version": "1.0.0",
  "description": "Node.js + Express.js + TypeScript template project.",
  "main": "./src/app.ts",
  "scripts": {
    "status": "tsc --p tsconfig.json --listFiles",
    "test": "jest",
    "start": "node -r module-alias/register ./dist/app.js",
    "build": "tsc -p ./tsconfig.prod.json",
    "dev": "nodemon --watch 'src/**/*.ts' --exec ts-node -r tsconfig-paths/register src/app.ts"
  },
  "_moduleAliases": {
    "@/routes": "dist/routes",
    "@/data": "dist/data",
    "@/middleware": "dist/middleware",
    "@/controllers": "dist/controllers",
    "@/db": "dist/db",
    "@/utils": "dist/utils"
  },
  "author": "huynhnd",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.617.0",
    "@aws-sdk/s3-request-presigner": "^3.617.0",
    "aws-sdk": "^2.1663.0",
    "@vercel/node": "^3.2.3",
    "babel": "^6.23.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "data-source": "^5.3.2",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "ejs": "~2.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "hpp": "^0.2.3",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^2.10.0",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.3",
    "xss": "^1.0.14",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.19.39",
    "@types/pg": "^8.11.6",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}